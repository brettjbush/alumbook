<dl class="tabs" data-tab data-options="deep_linking: true">
  <dd>
    <%= link_to 'List', controller: 'logins' %>
  </dd>
  <dd>
    <%= link_to 'Add', new_login_path %>
  </dd>
</dl>
<br>
<%= form_for :login, url: logins_path do |f| %>

 <div class="large-2">

  <p>
    <%= f.label :"First Name" %>
    <%= f.text_field :First_name%>
    <%= f.label :"Middle Initial" %>
    <%= f.text_field :mi%>

    <%= f.label :"Last Name" %>
    <%= f.text_field :Last_name%>

    <%= f.label :"User Name" %>
    <%= f.text_field :User_name%>

    <%= f.label :"Password" %>
    <%= f.text_field :Password%>

    <%= f.label :"Type" %>
    <%= f.select :Type, [['User', 'user'], ['Admin', 'admin'], ['Super User', 'SU']]%>
  </p>

  <p>
    <%= f.submit %>
  </p>
</div>

<% end %>
