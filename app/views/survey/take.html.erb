
<div class="row">
  <div class="large-12 columns">
    <table>
      <tr>


      




<%= form_for :user_survey_response, url: survey_index_path do |form|%>

  <% @question.each do |q|%>
  <%=fields_for :user_survey_response  do |f|%>


  <br><br><%=q.text%>

  <%if q.type=="short_answer"%>
  <%= render "short", {f: f, q: q} %>
  <%end%>
  <%if q.type=="single_choice"%>
  <%= render "single", {f: f, q: q} %>
  <%end%>
  <%if q.type=="multi_choice"%>
  <%= render "multi", {f: f, q: q} %>
  <%end%>
  <%if q.type=="sub_questions"%>
  <%= render "sub", {f: f, q: q} %>
  <%end%>

  <%end%>
  <%end%>
  <%=form.submit%>
  <%end%>
