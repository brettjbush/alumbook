<dl class="tabs" data-tab data-options="deep_linking: true">
  <dd>
    <%= link_to 'List', controller: 'logins' %>
  </dd>
  <dd>
    <%= link_to 'Add', new_login_path %>
  </dd>
</dl>
<br>
<%= form_for :login, url: logins_path do |f| %>

 <div class="large-2">

  <p>
    <%= f.label :"First Name" %>
    <%= f.text_field :first_name%>
    <%= f.label :"Middle Initial" %>
    <%= f.text_field :middle_initial%>

    <%= f.label :"Last Name" %>
    <%= f.text_field :last_name%>

    <%= f.label :"User Name" %>
    <%= f.text_field :username%>

    <%= f.label :"Password" %>
    <%= f.text_field :password%>

    <%= f.label :"Type" %>
    <%= f.select :type, [['User', 'user'], ['Admin', 'admin'], ['Worker', 'worker']]%>
  </p>

  <p>
    <%= f.submit %>
  </p>
</div>

<% end %>
