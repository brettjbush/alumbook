<dl class="tabs" data-tab data-options="deep_linking: true">
  <dd>
    <%= link_to 'List', controller: 'logins' %>
  </dd>
  <dd>
    <%= link_to 'Add', new_login_path %>
  </dd>
</dl>
 <br>

<%= form_for :login, url: login_path(@login), method: :patch do |f| %>

  <% if @login.errors.any? %>
    <div id="error_explanation">
      <h2>
        <%= pluralize(@login.errors.count, "error") %> prohibited
        this login from being saved:
      </h2>
      <ul>
        <% @login.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
 <div class="large-2">
<p>
    <%= f.label :First_name %>
    <%= f.text_field :first_name%>

    <%= f.label :Middle_Initial%>
    <%= f.text_field :middle_initial%>

    <%= f.label :Last_name %>
    <%= f.text_field :last_name%>

    <%= f.label :User_name %>
    <%= f.text_field :username%>

    <%= f.label :Password %>
    <%= f.text_field :password%>

    <%= f.label :Type %>
    <%= f.select :type, [['User', 'user'], ['Admin', 'admin'], ['Worker', 'worker']]%>
</p>
 </div>
  <p>
    <%= f.submit %>
  </p>

<% end %>
